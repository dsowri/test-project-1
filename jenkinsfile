pipeline {
    agent {
        label "sonarqube-node-1"
    }
    stages {
        stage('SCM') {
            steps {
                git 'https://github.com/dsowri/test-project-1.git'
            }
        }
        stage('Build and SonarQube Analysis') {
            steps {
                withSonarQubeEnv('My SonarQube Server') {
                    sh 'mvn clean package sonar:sonar'
                }
            }
        }
    }
}


