pipeline {
    agent {
        label "sonarqube-node-1"
    }
    stages {
        stage('SCM') {
            steps {
                git branch: 'main', url: 'https://github.com/dsowri/test-project-1.git'
            }
        }
        stage('Build and SonarQube Analysis') {
            steps {
                withSonarQubeEnv('sonarqube-1') {
                    sh 'mvn clean package sonar:sonar'
                }
            }
        }
    }
}


